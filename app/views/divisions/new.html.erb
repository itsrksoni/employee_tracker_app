<div>
    <h2>Create New Division</h2> <br/>

    <%= form_with(model: @division) do |f| %> 
        <ul>
            <% @division.errors.full_messages.each do |error| %>
            <li class="error-message"><%= error %></li>
            <% end %>
        </ul>
        <%= f.label :name %>
        <%= f.text_field :name %> <br/>
        <%= f.submit %> <br/>
    <% end %>
</div>
<button type="button" class="btn btn-primary"><%= link_to "Back", :back , class:"btn" %></button>